
<h2>Sign in</h2>
<table>
<%= form_for(resource, :as => resource_name, :url => session_path(resource_name)) do |f| %>
 <tr><td>
  <p><%= f.label :email %><br />
  <%= f.text_field :email %></p></td></tr>
 <tr><td>
  <p><%= f.label :password %><br />
  <%= f.password_field :password %></p></td></tr>
 <tr><td>
  <% if devise_mapping.rememberable? -%>
    <p><%= f.check_box :remember_me %> <%= f.label :remember_me %></p></td></tr>
  <% end -%>
 <tr><td>
  <p><%= f.submit "Sign in" %></p></td></tr>
<% end %>
</table>
<%= render :partial => "devise/shared/links" %>