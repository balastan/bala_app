          <li id="current"><a href="/"><span>Home</span></a></li>
          <li><a href="#"><span>Services</span></a></li>
          <li><a href="#"><span>News </span></a></li>
          <li><a href="#"><span>Contact Us</span></a></li>
          <li><a href="#"><span>About Us</span></a></li>
          
        <% if user_signed_in? %>
		  <li>Logged in as <strong><%= current_user.email %></strong>.</li>
		  <li><%= link_to edit_user_registration_path, :class => 'navbar-link'  do %> <span>Edit profile</span><%end%></li> 
		  <li><%= link_to destroy_user_session_path, method: :delete, :class => 'navbar-link'  do %> <span>Logout</span><%end%></li>
		<% else %>
		  <li><%= link_to new_user_registration_path, :class => 'navbar-link'  do %> <span>Sign up</span><%end%></li> 
